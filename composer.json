{
    "name": "xcentric-it-foundation/laravel-crud-controller",
    "description": "",
    "keywords": [
        "xcentric-it-foundation",
        "laravel-crud-controller"
    ],
    "homepage": "https://gitlab.com/xcentric-it-foundation/laravel-crud-controller",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dusan Jankovic",
            "email": "info@xcentric.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "spatie/laravel-query-builder": "^3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "XcentricItFoundation\\LaravelCrudController\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XcentricItFoundation\\LaravelCrudController\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-filter": "vendor/bin/phpunit --filter"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "XcentricItFoundation\\LaravelCrudController\\LaravelCrudControllerServiceProvider"
            ]
        }
    }
}
