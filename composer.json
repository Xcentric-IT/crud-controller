    "extra": {
        "laravel": {
            "providers": [
                "XcentricItFoundation\\LaravelCrudController\\LaravelCrudControllerServiceProvider"
            ],
            "aliases": {
                "LaravelCrudController": "XcentricItFoundation\\LaravelCrudController\\LaravelCrudControllerFacade"
            }
        }
