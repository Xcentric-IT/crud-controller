{
    "name": "xcentric-it/laravel-crud-controller",
    "description": "",
    "keywords": [
        "xcentric-it",
        "laravel-crud-controller"
    ],
    "homepage": "https://gitlab.com/xcentric-it/bizhive/laravel-crud-controller",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dusan Jankovic",
            "email": "info@xcentric.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "spatie/laravel-query-builder": "^5.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^2.1",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "XcentricItFoundation\\LaravelCrudController\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XcentricItFoundation\\LaravelCrudController\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "XcentricItFoundation\\LaravelCrudController\\LaravelCrudControllerServiceProvider"
            ]
        }
    }
}
