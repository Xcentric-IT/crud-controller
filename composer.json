{
    "name": "xcentricitfoundation/laravel-crud-controller",
    "description": "",
    "keywords": [
        "xcentricitfoundation",
        "laravel-crud-controller"
    ],
    "homepage": "https://gitlab.com/xcentric-it-foundation/laravel-crud-controller",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dusan Jankovic",
            "email": "info@xcentric.it",
            "role": "Developer"
        }
